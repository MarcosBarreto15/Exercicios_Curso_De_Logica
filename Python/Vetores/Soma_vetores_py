N = int(input("Quantos valores vai ter cada vetor? "))

A = [0 for x in range(N)]
B = [0 for x in range(N)]
C = [0 for x in range(N)]

print("Digite os valores de A:")

for i in range(0, N):
    A[i] = int(input())

print("Digite os valores de B:")

for i in range(0, N):
    B[i] = int(input())

for i in range(0, N):
    C[i] = A[i] + B[i]

print("VETOR RESULTANTE: ")

for i in range(0, N):
    print(C[i])

